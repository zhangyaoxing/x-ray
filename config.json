{
    "healthcheck": {
        "checksets": {
            "default": {
                "items": [
                    "BuildInfoItem",
                    "ClusterItem",
                    "ServerStatusItem",
                    "ShardKeyItem",
                    "HostInfoItem",
                    "SecurityItem",
                    "CollInfoItem",
                    "IndexInfoItem"
                ]
            },
            "test": {
                "items": [
                    "CollInfoItem",
                    "ServerStatusItem"
                ]
            }
        },
        "item_config": {
            "BuildInfoItem": {
                "eol_version": [
                    6,
                    0,
                    0
                ]
            },
            "CollInfoItem": {
                "obj_size_kb": 32,
                "collection_size_gb": 2048,
                "fragmentation_ratio": 0.5,
                "index_size_ratio": 0.2,
                "op_latency_ms": 100
            },
            "IndexInfoItem": {
                "unused_index_days": 7,
                "num_indexes": 10
            },
            "ClusterItem": {
                "replication_lag_seconds": 60,
                "oplog_window_hours": 48
            },
            "ServerStatusItem": {
                "used_connection_ratio": 0.8,
                "query_targeting": 1000,
                "query_targeting_obj": 1000,
                "cache_read_into_mb": 100,
                "updates_ratio": [
                    0.08,
                    0.1
                ],
                "dirty_ratio": [
                    0.15,
                    0.2
                ],
                "cache_fill_ratio": [
                    0.9,
                    0.95
                ]
            },
            "ShardKeyItem": {
                "sharding_imbalance_percentage": 0.1
            }
        },
        "template": "healthcheck/full.html"
    },
    "log": {
        "sample_rate": 1.0,
        "logsets": {
            "default": {
                "items": [
                    "ConnectionRateItem",
                    "SlowRateItem",
                    "ClientMetaItem",
                    "TopSlowItem",
                    "SlowChartItem",
                    "StateTraceItem",
                    "WEFItem",
                    "InfoItem"
                ]
            }
        },
        "item_config": {
            "WEFItem": {
                "ai_support": "none"
            },
            "TopSlowItem": {
                "top": 10
            }
        },
        "template": "log/full.html"
    }
}